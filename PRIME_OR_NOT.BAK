//A NUMBER IS PRIME OR NOT

#include<stdio.h>
#include<conio.h>
#include<math.h>

void main()
{
	int num,i;
	clrscr();

	printf("\nINPUT THE NUMBER: ");
	scanf("%d",num);

	for(i=2;i<=sqrt(num);i++)
	{
