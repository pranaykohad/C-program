//GCD USING RECURSION

#include<stdio.h>
#include<conio.h>

int gcd(int,int);

void main()
{
	int a,b;
	clrscr();

	printf("\n enter 2 numbers: ");
	scanf("%d %d",&a,&b);

	printf("\n GCD: %d",gcd(a,b));

	getch();
}

int gcd(int x,int y)
{
	if(x>y)
		return(x-y,y);

	else if(y>x)
		return(y-x,y);

	else
		return x;
}









































