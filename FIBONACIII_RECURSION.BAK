//FIBONACII SERIES USING RECURSION

#include<stdio.h>
#include<conio.h>

int fib(int);

void main()
{
	int nterm,i;
	clrscr();

	printf("\nENTER NTERM: ");
	scanf("%d",&nterm);

	for(i=0;i<nterm;i++)
		printf(" %d ",i);

	getch();
}

int fib(int num1)
{
	if(num1==0||num1==1)
		return 1;

	else
		return (fib(num1-1) + fib(num1-2));
}










































